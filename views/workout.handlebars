  <div class="box posAll">
    <div class="boxHeader">Workout Database</div>
    {{!-- SINGLE EXAMPLE --}}
    <div class="workoutOptions">
      <div class="option out" data-index="0">Full Body</div>
      <div class="option out" data-index="1">Upper Body</div>
      <div class="option out" data-index="2">Lower Body</div>
      <div class="option out" data-index="3">Back</div>
      <div class="option out" data-index="4">Biceps</div>
      <div class="option out" data-index="5">Triceps</div>
      <div class="option out" data-index="6">Chest</div>
      <div class="option out" data-index="7">Shoulders</div>
      <div class="option out" data-index="8">Legs</div>
    </div>
    <div class="largeContainer">
    {{#each fb}}
    <div class="workoutContainer">
      <ul>
      <p style="text-transform: uppercase">
          {{name}}
        </p>

        <div class="workout">
          <img class="workoutPic" style="order: 1" src="{{start}}" alt="squatEX1">
          <img class="workoutPic" style="order: 2" src="{{end}}" alt="squatEX2">
          <div class="workoutDesc">
            <p>EQUIPMENT: {{equipment}}</p>
            <hr>
            <p>TYPE: {{type}}</p>
          </div>

          <div class="workoutSet">
            <p>
              SETS: {{set}}
            </p>
            <hr>
            <p>
              REPS: {{rep}}
            </p>
          </div>

        </div>

      </ul>
      
    </div>
    
    {{/each}}
    </div>
  </div>
