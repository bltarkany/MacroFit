<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="../public/styles/workoutButton.css">
  </head>
<body>
  <div class="box posAll">
  <div class="boxHeader">Workout Database</div>
  {{!-- SINGLE EXAMPLE --}}
    <ul>
    <button class="btn fullbody">Full Body</button>
    <button class="btn upperbody">Upper Body</button>
    <button class="btn lowerbody">Lower Body</button>
    <button class="btn back">Back</button>
    <button class="btn biceps">Biceps</button>
    <button class="btn triceps">Triceps</button>
    <button class="btn chest">Chest</button>
    <button class="btn shoulders">Shoulders</button>
    <button class="btn legs">Legs</button>
    </ul>
  {{#each fb}}
  <div class="workoutContainer">

    <ul>
    <p style="text-transform: uppercase">
    {{name}}
    </p>

    <div class="workout">
      <div class="workoutPicDiv">
        <img class="workoutPic" src="{{start}}" alt="squatEX1">
      </div>
      <img class="workoutPic" src="{{end}}" alt="squatEX2">
      <div class="workoutDesc">
        <p>EQUIPMENT: {{equipment}}</p>
        <hr>
        <p>TYPE: {{type}}</p>
      </div>

      <div class="workoutSet">
        <p>
          SETS: {{set}}
        </p>
        <hr>
        <p>
          REPS: {{rep}}
        </p>
      </div>

    </div>

    </ul>
  </div>
  {{/each}}
</div>
<script src="jquery-3.4.1.min.js"></script>
<script src="../public/js/data.js"></script>
</body>
</html>